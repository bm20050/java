package edu.mission;

public class Test111 {

	public static void main(String[] args) {
		int[] numbers = {2, 3, 3, 5};
		int[] answer = {};
		for (int i = 0; i < numbers.length; i++) {
			for (int j = i + 1; j < numbers.length; j++) {
				if (numbers[i] < numbers[j]) {
					answer[i] = numbers[j];
					break;
				}
				answer[i] = -1;
			}
		}
		System.out.println(answer); 
	}

}

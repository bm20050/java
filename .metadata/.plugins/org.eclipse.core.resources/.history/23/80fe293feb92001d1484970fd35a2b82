package edu.mission;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double x;
		double a = -9.81;
		double t = 5;
		double v0 = 0.0;
		double x0 = 1000.0;
		
		x = 1/2 * a * t * t + v0 * t + x0;
		
		System.out.println(t + "초 후 위치 : " + x);
	}	

}

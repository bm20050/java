package com.ruby.java.ch11;

import java.io.*;

public class Test04 {

	public static void main(String[] args) {
		FileInputStream fi = null;
		try {
			fi = new FileInputStream("a.txt");
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		int c = fi.read();
		System.out.println((char) c);

	}

}
